<p style="color: green"><%= notice %></p>

<h1>Events</h1>

<div id="events" class="flexVertCenter">
  <% @events.each do |event| %>
    <%= render event %>
    <p>
      <%= link_to "See More", event, class: "selectBtn" %>
    </p>
  <% end %>
</div>
<% if current_user %>
  <%= link_to "New event", new_event_path, class: "selectBtn" %>
<% end %>
